    "lucide-react": "^0.468.0",
    "next": "15.5.4",
    "next-mdx-remote": "5.0.0",
    "next-themes": "^0.4.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rehype-autolink-headings": "7.1.0",
